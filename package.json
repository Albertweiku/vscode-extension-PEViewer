{
  "name": "pe-elf-viewer",
  "displayName": "PE/ELF Viewer",
  "description": "Professional PE (Windows executables), ELF (Linux shared objects), and LIB (COFF Archive) file structure viewer and analyzer with symbol decoding support",
  "version": "2.0.2",
  "publisher": "Albertweiku",
  "license": "MIT",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Albertweiku/vscode-extension-PEViewer.git"
  },
  "bugs": {
    "url": "https://github.com/Albertweiku/vscode-extension-PEViewer.git/issues"
  },
  "homepage": "https://github.com/Albertweiku/vscode-extension-PEViewer/blob/main/README.zh-cn.md",
  "keywords": [
    "PE",
    "ELF",
    "LIB",
    "exe",
    "dll",
    "so",
    "lib",
    "COFF",
    "archive",
    "binary",
    "executable",
    "analyzer",
    "viewer",
    "reverse engineering",
    "symbols",
    "imports",
    "exports",
    "sections",
    "HarmonyOS",
    "Linux",
    "Windows"
  ],
  "engines": {
    "vscode": "^1.60.0"
  },
  "capabilities": {
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "pe",
        "aliases": [
          "PE",
          "Portable Executable"
        ],
        "extensions": [
          ".exe",
          ".dll",
          ".ocx",
          ".sys",
          ".scr",
          ".drv",
          ".cpl"
        ]
      },
      {
        "id": "elf",
        "aliases": [
          "ELF",
          "Executable and Linkable Format"
        ],
        "extensions": [
          ".so",
          ".o",
          ".a",
          ".ko",
          ".elf"
        ]
      },
      {
        "id": "lib",
        "aliases": [
          "LIB",
          "COFF Archive"
        ],
        "extensions": [
          ".lib"
        ]
      }
    ],
    "customEditors": [
      {
        "viewType": "peviewer.peViewer",
        "displayName": "PEViewer",
        "selector": [
          {
            "filenamePattern": "*.exe"
          },
          {
            "filenamePattern": "*.dll"
          },
          {
            "filenamePattern": "*.ocx"
          },
          {
            "filenamePattern": "*.sys"
          },
          {
            "filenamePattern": "*.scr"
          },
          {
            "filenamePattern": "*.drv"
          },
          {
            "filenamePattern": "*.cpl"
          },
          {
            "filenamePattern": "*.so"
          },
          {
            "filenamePattern": "*.so.*"
          },
          {
            "filenamePattern": "lib*.so"
          },
          {
            "filenamePattern": "*.o"
          },
          {
            "filenamePattern": "*.a"
          },
          {
            "filenamePattern": "*.ko"
          },
          {
            "filenamePattern": "*.elf"
          },
          {
            "filenamePattern": "*.axf"
          },
          {
            "filenamePattern": "*.bin"
          },
          {
            "filenamePattern": "*.out"
          },
          {
            "filenamePattern": "*.lib"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "peviewer.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "package-vsix": "vsce package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.3",
    "@types/vscode": "^1.60.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "eslint": "^9.39.1",
    "prettier": "^3.7.4",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.3",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "elf-tools": "^1.1.2",
    "elfy": "^1.0.0",
    "pe-parser": "^1.0.11"
  }
}
